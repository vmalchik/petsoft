generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pet {
  id        String   @id @default(cuid())
  name      String
  ownerName String
  imageUrl  String
  age       Int
  notes     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
